{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Sid": "AllowSSORoleAccess",
            "Effect": "Allow",
            "Principal": "*",
            "Action": [
                "s3:GetObject",
                "s3:ListBucket"
            ],
            "Resource": [
                "arn:aws:s3:::aws-orf-social-media-analytics",
                "arn:aws:s3:::aws-orf-social-media-analytics/*"
            ]
        }
    ]
}
